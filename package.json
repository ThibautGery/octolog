{
  "name": "cv-octo",
  "version": "0.0.1",
  "description": "CV generator",
  "main": "basics.js",
  "dependencies": {
    "dynamodb": "1.0.0",
    "joi": "10.4.1",
    "lodash": "4.17.4",
    "promisify-node": "0.4.0",
    "request": "2.81.0",
    "request-promise-native": "1.0.3",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "dredd": "3.4.3",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "nock": "9.0.13",
    "proxyquire": "1.7.11",
    "serverless-offline": "3.13.3",
    "sinon": "2.1.0"
  },
  "scripts": {
    "runDynamo": "java -Djava.library.path=./DynamoDBLocal_lib -jar DynamoDBLocal.jar -sharedDb",
    "dlDynamo": "curl https://s3.eu-central-1.amazonaws.com/dynamodb-local-frankfurt/dynamodb_local_latest.tar.gz | tar -x DynamoDBLocal.jar DynamoDBLocal_lib",
    "preback:dev": "DYNAMO_URL=http://localhost:8000 node ./lambda/dynamo/createTable",
    "back:dev": "DYNAMO_URL=http://localhost:8000 sls offline start",
    "back:unit": "./node_modules/.bin/istanbul cover --print details _mocha -- -R spec lambda/tests/** --recursive",
    "back:e2e": "OCTOPOD_URL=http://private-08aab-octopodmock.apiary-mock.com ./node_modules/.bin/dredd",
    "back:test": "npm run back:unit && npm run back:e2e",
    "test": "npm run back:test"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.octo.com:australia/cv-generator-back.git"
  },
  "author": "tgery",
  "license": "ISC"
}
